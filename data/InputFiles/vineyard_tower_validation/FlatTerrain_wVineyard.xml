<QESinput>
<simulationParameters>
  <domain> 250 150 150</domain>
  <cellSize> 0.2 0.2 0.1 </cellSize> <!-- Meters-->
  <verticalStretching> 0 </verticalStretching>
  <totalTimeIncrements> 1 </totalTimeIncrements>
  <UTCConversion> 0 </UTCConversion>
  <Epoch> 1510930800 </Epoch>
  <rooftopFlag> 0 </rooftopFlag>
  <upwindCavityFlag> 0 </upwindCavityFlag>
  <streetCanyonFlag> 0 </streetCanyonFlag>
  <streetIntersectionFlag> 0 </streetIntersectionFlag>
  <wakeFlag> 0 </wakeFlag>
  <sidewallFlag> 0 </sidewallFlag>
  <maxIterations> 500 </maxIterations>
  <residualReduction> 3 </residualReduction>
  <meshTypeFlag> 0 </meshTypeFlag> <!-- 0 is stair; 1 is cut-cell  -->
  <useDiffusion> 0 </useDiffusion>
  <domainRotation> 0 </domainRotation>
  <UTMX> 0 </UTMX>
  <UTMY> 0 </UTMY>
  <UTMZone> 1 </UTMZone>
  <UTMZoneLetter> 17 </UTMZoneLetter>
</simulationParameters>
<metParams>
  <z0_domain_flag> 0 </z0_domain_flag>            <!-- Distribution of sueface roughness for domain (0-uniform, 1-custom -->
  <sensor>
	<site_coord_flag> 1 </site_coord_flag> 				<!-- Sensor site coordinate system (1=QUIC, 2=UTM, 3=Lat/Lon) -->
	<site_xcoord> 1.0</site_xcoord>
	<site_ycoord> 1.0</site_ycoord>		
	<site_UTM_x> 2.0 </site_UTM_x> 								<!-- x components of site coordinate in UTM -->
	<site_UTM_y> 2.0 </site_UTM_y> 								<!-- y components of site coordinate in UTM -->
	<site_UTM_zone> 0 </site_UTM_zone> 						<!-- UTM zone of the sensor site -->
	<timeSeries>
	  <boundaryLayerFlag> 1 </boundaryLayerFlag>
	  <siteZ0> 0.03 </siteZ0>
	  <reciprocal> 0 </reciprocal>
	  <height> 10.1 </height>
	  <speed> 3.5 </speed>
	  <direction> 270 </direction> 
    </timeSeries>
  </sensor>
</metParams>
<turbParams>
  <method>0</method>
  <samples>2000</samples>
  <save>false</save>
  <backgroundMixing>0.0</backgroundMixing>
</turbParams>
<canopies>
  <num_canopies>1</num_canopies>
  <Vineyard>
    <opticalPorosity>0.1157</opticalPorosity>
    <height>2.16</height>
    <understoryHeight>0.5</understoryHeight>
    <rowSpacing>2.5</rowSpacing>
    <rowWidth>0.5</rowWidth>
    <rowAngle>0</rowAngle>
    <corner1x> 6 </corner1x>
    <corner1y> 2.5 </corner1y>
    <corner2x> 45 </corner2x>
    <corner2y> 2.5 </corner2y>
    <corner3x> 45 </corner3x>
    <corner3y> 27.5 </corner3y>
    <corner4x> 6 </corner4x>
    <corner4y> 27.5 </corner4y>
    <thinFence> 0 </thinFence>
    <upstreamSigmaW> 0.4095 </upstreamSigmaW>
    <upstreamUstar> 0.4472 </upstreamUstar>
    <displacementHeight> 1.42</displacementHeight>
  </Vineyard>
</canopies>
<fileOptions>
  <outputFlag>1</outputFlag>
  <outputFields>u</outputFields> 
  <outputFields>v</outputFields> 
  <outputFields>w</outputFields>
  <outputFields>icell</outputFields>  
  <outputTURBInputFile>true</outputTURBInputFile>
  <massConservedFlag> 0 </massConservedFlag>
  <sensorVelocityFlag> 0 </sensorVelocityFlag>
  <staggerdVelocityFlag> 0 </staggerdVelocityFlag>
</fileOptions>
</QESinput>
