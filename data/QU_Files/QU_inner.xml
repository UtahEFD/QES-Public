<simulationParameters>

	<!--SHP>../scratch/seoul_small_cut.shp</SHP-->				<!-- Address to shapefile location-->
	<!--SHPBuildingLayer>seoul_small_cut</SHPBuildingLayer-->

	<halo_x> 0.0 </halo_x>					<!-- Halo region added to x-direction of domain (at the beginning and the end of domain)-->
  	<halo_y> 0.0 </halo_y>					<!-- Halo region added to y-direction of domain (at the beginning and the end of domain)-->

	<domain> 200 200 200 </domain>
	<cellSize> 1.0 1.0 1.0 </cellSize> <!-- Meters-->
	<verticalStretching> 0 </verticalStretching>              <!-- vertical stretching (0-uniform grid, 1-costum grid)-->

	<totalTimeIncrements> 0 </totalTimeIncrements> 				<!-- Number of time steps-->
	<rooftopFlag> 0 </rooftopFlag> 											<!-- Rooftop flag (0-none, 1-log profile, 2-vortex) -->
	<upwindCavityFlag> 2 </upwindCavityFlag> 						<!-- Upwind cavity flag (0-none, 1-Rockle, 2-MVP, 3-HMVP) -->
	<streetCanyonFlag> 4 </streetCanyonFlag> 						<!-- Street canyon flag (0-none, 1-Roeckle, 2-CPB, 3-exp. param. PKK, 4-Roeckle w/ Fackrel) -->
	<streetIntersectionFlag> 0 </streetIntersectionFlag> 		<!--Street intersection flag (0-off, 1-on) -->
	<wakeFlag> 2 </wakeFlag> 												<!-- Wake flag (0-none, 1-Rockle, 2-Modified Rockle, 3-Area Scaled) -->
	<sidewallFlag> 1 </sidewallFlag> 								<!-- Sidewall flag (0-off, 1-on) -->
	<maxIterations> 500 </maxIterations> 						<!-- Maximum number of iterations -->
	<residualReduction> 3 </residualReduction> 			<!-- Residual Reduction (Orders of Magnitude) -->
	<meshTypeFlag> 0 </meshTypeFlag> 								<!-- Type of meshing scheme (0-Stair step (origional QUIC), 1-Cut-cell method -->
	<useDiffusion> 0 </useDiffusion>
	<domainRotation> 0 </domainRotation>  					<!-- Rotation angle of domain relative to true north -->
	<UTMx> 0 </UTMx> 														<!-- x component (m) in UTM -->
	<UTMy> 0 </UTMy> 														<!-- y component (m) in UTM -->
	<UTMZone> 1 </UTMZone> 												<!-- UTM zone that domain located -->
	<UTMZoneLetter> 17 </UTMZoneLetter>
</simulationParameters>                     <!-- End of simulation parameters -->
<metParams>
	<metInputFlag> 0 </metInputFlag>
	<num_sites> 1 </num_sites>
	<maxSizeDataPoints> 1 </maxSizeDataPoints>
	<siteName> sensor1 </siteName>
	<fileName> sensor1.inp </fileName>
	<z0_domain_flag> 0 </z0_domain_flag>            <!-- Distribution of sueface roughness for domain (0-uniform, 1-custom -->
  <sensor>
    <site_coord_flag> 1 </site_coord_flag> 				<!-- Sensor site coordinate system (1=QUIC, 2=UTM, 3=Lat/Lon) -->
    <site_xcoord> 1.0  </site_xcoord> 					<!-- x component of site location in QUIC domain (m) (if site_coord_flag = 1) -->
    <site_ycoord> 1.0 </site_ycoord> 				<!-- y component of site location in QUIC domain (m) (if site_coord_flag = 1)-->
    <site_UTM_x> 2.0 </site_UTM_x> 								<!-- x components of site coordinate in UTM (if site_coord_flag = 2) -->
    <site_UTM_y> 2.0 </site_UTM_y> 								<!-- y components of site coordinate in UTM (if site_coord_flag = 2)-->
    <site_UTM_zone> 0 </site_UTM_zone> 						<!-- UTM zone of the sensor site (if site_coord_flag = 2)-->

    <boundaryLayerFlag> 1 </boundaryLayerFlag> 			<!-- Site boundary layer flag (1-log, 2-exp, 3-urban canopy, 4-data entry) -->
    <siteZ0> 0.1 </siteZ0> 									<!-- Site z0 -->
    <reciprocal> 0.0 </reciprocal> 						<!-- Reciprocal Monin-Obukhov Length (1/m) -->
    <height> 10.0 </height> 										<!-- Height of the sensor -->
    <speed> 5.0 </speed> 											<!-- Measured speed at the sensor height -->
    <direction> 360.0 </direction> 						<!-- Wind direction of sensor -->
  </sensor>                       	<!-- Wnd of sensor section -->

</metParams>								<!-- End of meteorological parameters -->
