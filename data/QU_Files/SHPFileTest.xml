	<!--site_xcoord> 0.0 </site_xcoord--> 						<!-- x component of site location in QUIC domain (m) -->
	<!--site_ycoord> 12.5 </site_ycoord--> 					<!-- y component of site location in QUIC domain (m) -->
	<!--boundaryLayerFlag> 1 </boundaryLayerFlag--> 				<!-- Site boundary layer flag (1-log, 2-exp, 3-urban canopy) -->
	<!--siteZ0> 0.01 </siteZ0--> 										<!-- Site z0 -->
	<!--reciprocal> 0.0 </reciprocal--> 						<!-- Reciprocal Monin-Obukhov Length (1/m) -->
	<!--height> 1.0 </height--> 										<!-- Height of the sensor -->
	<!--speed> 3.0 </speed--> 											<!-- Measured speed at the sensor height -->
	<!--direction> 270.0 </direction--> 						<!-- Wind direction of sensor -->

	<!--canopyHeight> 10.0 </canopyHeight-->     <!-- Canopy height -->
	<!--attenuationCoefficient> 1.97 </attenuationCoefficient--> 				<!-- Canopy attenuation coefficient -->
	<!--/sensor-->
</metParams>  															<!-- End of meteorological parameters -->
